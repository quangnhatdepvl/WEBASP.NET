<!DOCTYPE html>
<html>
  <head>
    <title>JSON data to HTML table</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.js"/>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  </head>
  <body>
    <div class="container">
      <div class="table-responsive">
        <h1>Data Fetch into HTML Table using Ajax Jquery getJSON method</h1>
        <br />
        <table class="table table-bordered table-striped" id="employee_table">
          <tr>
            <th>Name</th>
            <th>Username</th>
            <th>Email</th>
            <th>Address</th>
             <th>geo</th>
             <th>phone</th>
             <th>website</th>
             <th>company</th>
                  </tr>
        </table>
        
    </div>
    </div>
     
  </body>
  
</html>
<script>
$(document).ready(function(){
    $.getJSON("http://localhost:62101/Home/Detail", function(data){
    var employee_data = '';
    $.each(data, function(key, value){
      employee_data += '<tr>';
      employee_data += '<td>'+value.Price+'</td>';
     
      employee_data += '<tr>';
    });
    $('#employee_table').append(employee_data);
  });
  
});
</script>